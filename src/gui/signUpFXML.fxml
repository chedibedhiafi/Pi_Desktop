<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="1200.0" stylesheets="@elements/css/SignIn.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.SignUpFXMLController">
   <children>
      <Pane prefHeight="800.0" prefWidth="600.0">
         <children>
            <MediaView fx:id="mediaView" fitHeight="800.0" fitWidth="600.0" />
         </children>
      </Pane>
      <Pane layoutX="600.0" prefHeight="800.0" prefWidth="600.0">
         <children>
            <TextField fx:id="loginTF" layoutX="150.0" layoutY="164.0" prefHeight="40.0" prefWidth="300.0" promptText="Insérez votre login">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </TextField>
            <Label fx:id="signin" layoutX="350.0" layoutY="51.0" onMouseClicked="#signin" text="Vous avez déja un compte ?" />
            <ImageView fitHeight="150.0" fitWidth="250.0" layoutX="25.0" layoutY="51.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@elements/images/logo%20pi%203.png" />
               </image>
            </ImageView>
            <TextField fx:id="prenomTF" layoutX="150.0" layoutY="237.0" prefHeight="40.0" prefWidth="300.0" promptText="Insérez votre prénom">
