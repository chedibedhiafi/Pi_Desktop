               </font>
            </TextField>
            <TextField fx:id="nomTF" layoutX="150.0" layoutY="315.0" prefHeight="40.0" prefWidth="300.0" promptText="Insérez votre nom">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </TextField>
            <TextField fx:id="emailTF" layoutX="150.0" layoutY="541.0" prefHeight="40.0" prefWidth="300.0" promptText="Insérez votre adresse mail">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </TextField>
            <DatePicker fx:id="dateDP" layoutX="150.0" layoutY="617.0" prefHeight="40.0" prefWidth="300.0" promptText="Insérez votre date de naissance" />
            <Button layoutX="225.0" layoutY="701.0" mnemonicParsing="false" onAction="#submitBT" prefHeight="50.0" prefWidth="150.0" text="S'inscrire" />
            <Label fx:id="loginMsg" layoutX="150.0" layoutY="204.0" prefHeight="17.0" prefWidth="300.0" styleClass="messagev" />
            <Label fx:id="prenomMsg" layoutX="150.0" layoutY="277.0" prefHeight="17.0" prefWidth="300.0" styleClass="messagev" />
            <Label fx:id="nomMsg" layoutX="150.0" layoutY="355.0" prefHeight="17.0" prefWidth="300.0" styleClass="messagev" />
            <Label fx:id="mdpMsg" layoutX="150.0" layoutY="429.0" prefHeight="17.0" prefWidth="300.0" styleClass="messagev" />
            <Label fx:id="mdpvMsg" layoutX="150.0" layoutY="502.0" prefHeight="17.0" prefWidth="300.0" styleClass="messagev" />
            <Label fx:id="emailMsg" layoutX="150.0" layoutY="581.0" prefHeight="17.0" prefWidth="300.0" styleClass="messagev" />
            <Label fx:id="dateMsg" layoutX="150.0" layoutY="657.0" prefHeight="17.0" prefWidth="300.0" styleClass="messagev" />
            <PasswordField fx:id="mdpTF" layoutX="150.0" layoutY="389.0" prefHeight="40.0" prefWidth="300.0" promptText="Insérez votre mot de passe" />
            <PasswordField fx:id="mdpverifTF" layoutX="150.0" layoutY="463.0" prefHeight="39.0" prefWidth="300.0" promptText="Confirmez votre mot de passe" />
